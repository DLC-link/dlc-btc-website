# VITE_ATTESTOR_API_URLS=
# VITE_BITCOIN_NETWORK=regtest
# VITE_BITCOIN_EXPLORER_API_URL=https://devnet.dlc.link/electrs
# VITE_BITCOIN_BLOCKCHAIN_API_URL=https://devnet.dlc.link/electrs

